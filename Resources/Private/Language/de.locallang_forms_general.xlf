        <body>
            <!-- TRANSLATION FOR ALL FORMS

                 Note: Every translation key can be overridden by prepending it with the form identifier:
                       '<form-identifier>.element.<element-name>.properties.label' will override
                       'element.<element-name>.properties.label'
            -->


            <!-- ### LABELS, FIELDSETS, HEADINGS
                 Translation keys:
                 - element.<element-name>.properties.label
                 - element.<element-type>.properties.<property-name>
            -->
            <trans-unit id="element.title.properties.label">
                <source>Title</source>
                <target>Anrede</target>
            </trans-unit>
            <trans-unit id="element.firstname.properties.label">
                <source>First name</source>
                <target>Vorname</target>
            </trans-unit>
            <trans-unit id="element.lastname.properties.label">
                <source>Last name</source>
                <target>Nachname</target>
            </trans-unit>
            <trans-unit id="element.email.properties.label">
                <source>Email address</source>
                <target>E-Mail-Adresse</target>
            </trans-unit>
            <trans-unit id="element.message.properties.label">
                <source>Message</source>
                <target>Ihre Nachricht</target>
            </trans-unit>
            <trans-unit id="element.SummaryPage.properties.label">
                <source>Summary</source>
                <target>Übersicht</target>
            </trans-unit>




            <!-- ### OPTIONS
                 Translation key: element.<element-name>.properties.options.<value>
            -->
            <trans-unit id="element.title.properties.prependOptionLabel">
                <source>Please choose ...</source>
                <target>Bitte wählen ...</target>
            </trans-unit>
            <trans-unit id="element.title.properties.options.male">
                <source>Mr</source>
                <target>Herr</target>
            </trans-unit>
            <trans-unit id="element.title.properties.options.female">
                <source>Ms</source>
                <target>Frau</target>
            </trans-unit>




            <!-- ### PLACEHOLDER ATTRIBUTES
                 Translation key: element.<element-name>.properties.placeholder
            -->
            <trans-unit id="element.firstname.properties.placeholder">
                <source>Your first name</source>
                <target>Ihr Vorname</target>
            </trans-unit>
            <trans-unit id="element.lastname.properties.placeholder">
                <source>Your last name</source>
                <target>Ihr Nachname</target>
            </trans-unit>
            <trans-unit id="element.email.properties.placeholder">
                <source>john@doe.com</source>
                <target>michael@mustermann.de</target>
            </trans-unit>




            <!-- ### BUTTONS
                 Translation keys:
                 - element.Form.renderingOptions.submitButtonLabel
                 - element.Page.renderingOptions.previousButtonLabel
                 - element.Page.renderingOptions.nextButtonLabel
                 - element.SummaryPage.renderingOptions.previousButtonLabel
                 - element.SummaryPage.renderingOptions.nextButtonLabel
            -->
            <trans-unit id="element.Form.renderingOptions.submitButtonLabel">
                <source>Send message</source>
                <target>Nachricht senden</target>
            </trans-unit>
            <trans-unit id="element.Page.renderingOptions.previousButtonLabel">
                <source>back</source>
                <target>zurück</target>
            </trans-unit>
            <trans-unit id="element.Page.renderingOptions.nextButtonLabel">
                <source>continue</source>
                <target>weiter</target>
            </trans-unit>
            <trans-unit id="element.SummaryPage.renderingOptions.previousButtonLabel">
                <source>back</source>
                <target>zurück</target>
            </trans-unit>
            <trans-unit id="element.SummaryPage.renderingOptions.nextButtonLabel">
                <source>continue</source>
                <target>weiter</target>
            </trans-unit>




            <!-- ### FINISHERS
                 Translation key: finisher.<finisher-identifier>.<option-name>
            -->
            <trans-unit id="finisher.EmailToSender.subject">
                <source>Your message to ACME Ltd.</source>
                <target>Ihre Nachricht an die ACME GmbH</target>
            </trans-unit>
            <trans-unit id="finisher.EmailToReceiver.subject">
                <source>Message from website</source>
                <target>Nachricht von Website</target>
            </trans-unit>
            <trans-unit id="finisher.Confirmation.message">
                <source>Thank you for your message! We will get back to you as soon as possible.</source>
                <target>Vielen Dank für Ihre Nachricht! Wir werden uns schnellstmöglich bei Ihnen melden.</target>
            </trans-unit>
